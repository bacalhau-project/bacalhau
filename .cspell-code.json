{
    "version": "0.2",
    "language": "en",
    "allowCompoundWords": true,
    "dictionaryDefinitions": [
        {
            "name": "custom-dictionary",
            "path": "./.cspell/custom-dictionary.txt",
            "addWords": true
        }
    ],
    "dictionaries": [
        "en",
        "custom-words",
        "custom-dictionary"
    ],
    "ignorePaths": [
        "**/package.json",
        "**/docs/package-lock.json",
        "**/docs/docs/examples/model-training/Stable-Diffusion-Dreambooth/index.md",
        "docs/docs/examples/model-training/Training-Tensorflow-Model/index.md",
        "./webui/build",
        "./webui/node_modules",
        "./webui/package.json",
        "./webui/package-lock.json",
        "./.gitprecommit",
        "./webui/tsconfig.json",
        "./vendor",
        "go.sum",
        "go.mod",
        "go.work.sum",
        "apps"
    ],
    "ignoreRegExpList": [
        "Urls",
        "Email",
        "RsaCert",
        "SshRsa",
        "Base64MultiLine",
        "Base64SingleLine",
        "CommitHash",
        "CommitHashLink",
        "CStyleHexValue",
        "CSSHexValue",
        "SHA",
        "HashStrings",
        "UnicodeRef",
        "UUID",
        "/github.com.*/",
        "/\\w+{12,}/"
    ]
}
