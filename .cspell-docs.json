{
    "version": "0.2",
    "language": "en",
    "allowCompoundWords": true,
    "dictionaryDefinitions": [
        {
            "name": "custom-dictionary",
            "path": "./.cspell/custom-dictionary.txt",
            "addWords": true
        }
    ],
    "dictionaries": [
        "en",
        "custom-words"
    ],
    "ignorePaths": [
        "**/package.json",
        "**/docs/package-lock.json",
        "**/docs/docs/examples/model-training/Stable-Diffusion-Dreambooth/index.md",
        "docs/docs/examples/model-training/Training-Tensorflow-Model/index.md",
        "./docs/swagger.json",
        "./docs/docusaurus.config.js",
        "./docs/sidebars.js",
        "**/*.css"
    ],
    "ignoreRegExpList": [
        "Urls",
        "Email",
        "RsaCert",
        "SshRsa",
        "Base64MultiLine",
        "Base64SingleLine",
        "CommitHash",
        "CommitHashLink",
        "CStyleHexValue",
        "CSSHexValue",
        "SHA",
        "HashStrings",
        "UnicodeRef",
        "UUID",
        "/github.com.*/",
        "/\\w+{12,}/",
        "/ipfs://\\w*/",
        "/Qm[\\w]+[^\\w]/"
    ]
}