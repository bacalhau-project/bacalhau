      },
      "InputSources": [
        {
          "Alias": "script",
          "Target": "/script.py",
          "Source": {
            "Type": "ipfs",
            "Params": {
              "CID": "QmT5NvUtoM5nXKJ74d4kJL8PFrEBz8jeQV3BgZWqkLqcD6"
            }
          }
        }
      ]
    }
  ]
}
