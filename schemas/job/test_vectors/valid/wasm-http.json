{
  "Name": "WASM Job with HTTP",
  "Type": "batch",
  "Namespace": "default",
  "Count": 1,
  "Tasks": [
    {
      "Name": "main",
      "Engine": {
        "Type": "wasm",
        "Params": {
          "EntryModule": {
            "Alias": "module",
            "Target": "/module.wasm",
            "Source": {
              "Type": "urlDownload",
              "Params": {
                "URL": "http://example.com/module.wasm"
              }
            }
          },
          "Entrypoint": "main",
          "Parameters": ["arg1", "arg2"]
        }
      }
    }
  ]
}
